{{aUserModel.name}}
<div>{{listOfUsers.firstObject.name}}</div>
{{if name "I have a name" "I have no name"}}
<span data-has-name={{if name true}}></span>
<div {{on "click" this.hello}} />

----------------------------------------------------

[
	["handlebars", [
		["delimiter", "{{"],
		["variable", "aUserModel"],
		["punctuation", "."],
		["variable", "name"],
		["delimiter", "}}"]
	]],

	["tag", [
		["punctuation", "<"],
		["tag", ["div"]],
		["punctuation", ">"]
	]],
	["handlebars", [
		["delimiter", "{{"],
		["variable", "listOfUsers"],
		["punctuation", "."],
		["variable", "firstObject"],
		["punctuation", "."],
		["variable", "name"],
		["delimiter", "}}"]
	]],
	["tag", [
		["punctuation", "</"],
		["tag", ["div"]],
		["punctuation", ">"]
	]],

	["handlebars", [
		["delimiter", "{{"],
		["variable", "if"],
		["variable", "name"],
		["string", "\"I have a name\""],
		["string", "\"I have no name\""],
		["delimiter", "}}"]
	]],

	["tag", [
		["punctuation", "<"],
		["tag", ["span"]],
		["attr-name", ["data-has-name"]],
		["attr-equals", "="],
		["handlebars", [
			["delimiter", "{{"],
			["variable", "if"],
			["variable", "name"],
			["boolean", "true"],
			["delimiter", "}}"]
		]],
		["punctuation", ">"]
	]],
	["tag", [
		["punctuation", "</"],
		["tag", ["span"]],
		["punctuation", ">"]
	]],

	["tag", [
		["punctuation", "<"],
		["tag", ["div"]],
		["handlebars", [
			["delimiter", "{{"],
			["variable", "on"],
			["string", "\"click\""],
			["variable", "this"],
			["punctuation", "."],
			["variable", "hello"],
			["delimiter", "}}"]
		]],
		["punctuation", "/>"]
	]]
]

